{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>acbgbca/renovate"
  ],
  "packageRules": [
    {
      "groupName": "all non-major dependencies",
      "groupSlug": "all-non-major",
      "matchPackageNames": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch",
        "digest"
      ]
    }
  ]
}
